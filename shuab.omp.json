{
    "blocks": [
      {
        "alignment": "left",
        "segments": [
          {
            "background": "#ea23e3",
            "foreground": "#ffffff",
            "leading_diamond": "\ue0b8 ",
            "style": "accordion",
            "powerline_symbol": " \ue0b0",
            "template": "\uf91c{{ .HostName }} ",
            "type": "aws"
          },
          {
            "background": "#8f023a",
            "foreground": "#ffffff",
            "powerline_symbol": "\ue0b0",
            "properties": {
              "home_icon": "\uf7db ",
              "folder_separator_icon": " <transparent>\ue0b1</> ",
              "folder_icon": "\uf115",
              "style": "agnoster"
            },
            "style": "powerline",
            "template": " {{ path .Path .Location }} ",
            "type": "path"
          },
          {
            "background": "#29f43d",
            "background_templates": [
              "{{ if (.Working.Changed) }}#FFEB3B{{ end }}",
              "{{ if .Staging.Changed }}#ffffff {{ end }}",
              "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#FFA300{{ end }}",
              "{{ if gt .Ahead 0 }}#FF7070{{ end }}",
              "{{ if gt .Behind 0 }}#90F090{{ end }}"
            ],
            "foreground": "#000000",
            "powerline_symbol": "\ue0b0",
            "properties": {
              "fetch_stash_count": true,
              "fetch_status": true,
              "fetch_upstream_icon": true
            },
            "style": "powerline",
            "template": " \uf113  {{ url .UpstreamIcon .UpstreamURL }}{{ .HEAD }}{{ if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }}<#f70014> \uf044 {{ .Working.String }}</>{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}<#0cae00> \uf046 {{ .Staging.String }}</>{{ end }}{{ if gt .StashCount 0 }} \uf692 {{ .StashCount }}{{ end }} ",
            "type": "git"
          }
        ],
        "type": "prompt"
      },
      {
        "alignment": "left",
        "newline": true,
        "segments": [
          {
            "foreground": "#FFF",
            "foreground_templates": [
              "{{ if gt .Code 0 }}#D51111{{ end }}"
            ],
            "style": "plain",
            "template": "\u0024",
            "type": "text"
          }
        ],
        "type": "prompt"
      }
    ],
    "console_title_template": "{{if .Root}} \u26a1 {{end}}{{.UserName}} \u2794 üìÅ {{.Folder}}",
    "final_space": true,
    "secondary_prompt": {
      "background": "#ec9706",
      "foreground": "#100e23",
      "template": " \uf061 ",
      "osc99": true
    },
    "version": 2
  }
